><span class=3D"c1"># custom_action.py</span>

<span class=3D"kn">import</span> <span class=3D"nn">argparse</span>

<span class=3D"k">class</span> <span class=3D"nc">VerboseStore</span><span =
class=3D"p">(</span><span class=3D"n">argparse</span><span class=3D"o">.</s=
pan><span class=3D"n">Action</span><span class=3D"p">):</span>
    <span class=3D"k">def</span> <span class=3D"fm">__call__</span><span cl=
ass=3D"p">(</span><span class=3D"bp">self</span><span class=3D"p">,</span> =
<span class=3D"n">parser</span><span class=3D"p">,</span> <span class=3D"n"=
>namespace</span><span class=3D"p">,</span> <span class=3D"n">values</span>=
<span class=3D"p">,</span> <span class=3D"n">option_string</span><span clas=
s=3D"o">=3D</span><span class=3D"kc">None</span><span class=3D"p">):</span>
        <span class=3D"nb">print</span><span class=3D"p">(</span><span clas=
s=3D"sa">f</span><span class=3D"s2">"Storing </span><span class=3D"si">{</s=
pan><span class=3D"n">values</span><span class=3D"si">}</span><span class=
=3D"s2"> in the </span><span class=3D"si">{</span><span class=3D"n">option_=
string</span><span class=3D"si">}</span><span class=3D"s2"> option..."</spa=
n><span class=3D"p">)</span>
        <span class=3D"nb">setattr</span><span class=3D"p">(</span><span cl=
ass=3D"n">namespace</span><span class=3D"p">,</span> <span class=3D"bp">sel=
f</span><span class=3D"o">.</span><span class=3D"n">dest</span><span class=
=3D"p">,</span> <span class=3D"n">values</span><span class=3D"p">)</span>

<span class=3D"n">parser</span> <span class=3D"o">=3D</span> <span class=3D=
"n">argparse</span><span class=3D"o">.</span><span class=3D"n">ArgumentPars=
er</span><span class=3D"p">()</span>

<span class=3D"n">parser</span><span class=3D"o">.</span><span class=3D"n">=
add_argument</span><span class=3D"p">(</span><span class=3D"s2">"-n"</span>=
