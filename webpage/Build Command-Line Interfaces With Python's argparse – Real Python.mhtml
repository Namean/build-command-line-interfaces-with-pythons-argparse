  <div style=3D"position: relative;">
    <div class=3D"highlight highlight--with-header"><pre><span></span><code=
><span class=3D"c1"># ls.py v3</span>

<span class=3D"kn">import</span> <span class=3D"nn">argparse</span>
<span class=3D"kn">import</span> <span class=3D"nn">datetime</span>
<span class=3D"kn">from</span> <span class=3D"nn">pathlib</span> <span clas=
s=3D"kn">import</span> <span class=3D"n">Path</span>

<span class=3D"hll"><span class=3D"n">parser</span> <span class=3D"o">=3D</=
span> <span class=3D"n">argparse</span><span class=3D"o">.</span><span clas=
s=3D"n">ArgumentParser</span><span class=3D"p">(</span><span class=3D"n">pr=
og</span><span class=3D"o">=3D</span><span class=3D"s2">"ls"</span><span cl=
ass=3D"p">)</span>
</span>
<span class=3D"c1"># ...</span>

<span class=3D"k">for</span> <span class=3D"n">entry</span> <span class=3D"=
ow">in</span> <span class=3D"n">target_dir</span><span class=3D"o">.</span>=
<span class=3D"n">iterdir</span><span class=3D"p">():</span>
    <span class=3D"nb">print</span><span class=3D"p">(</span><span class=3D=
"n">build_output</span><span class=3D"p">(</span><span class=3D"n">entry</s=
pan><span class=3D"p">,</span> <span class=3D"n">long</span><span class=3D"=
o">=3D</span><span class=3D"n">args</span><span class=3D"o">.</span><span c=
lass=3D"n">long</span><span class=3D"p">))</span>
</code></pre></div>
   =20
